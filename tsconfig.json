{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "strict": true,
      "esModuleInterop": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": "./src",
      "paths": {
        "*": ["types/*"]
      }
    },
    "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.json"],
    "exclude": ["node_modules"]
  }
  